#include "main.h"

/**
 * print_alphabet_x10 - prints 10 times the alphabet in lowercase
 *
 * Return: Always 0.
 */

int print_alphabet_x10(void)
{
	char letter;
	int i;

	i = 0;
	if (i<3)
	{
		for (letter = 'a'; letter <= 'z'; letter++)
		{
			_putchar(letter);
		}

		_putchar('\n');
		i++;
	}
	return (0);
}
