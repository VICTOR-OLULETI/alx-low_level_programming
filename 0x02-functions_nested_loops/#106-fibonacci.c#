#include "main.h"
#include "stdio.h"

/**
 * main - prints the first 98 fibonacci numbers
 *
 *
 *
 * Return: Always 0.
 */

int main(void)
{
	unsigned long fib1, fib2, sum;

	int i;

	fib1 = 1;
	fib2 = 2;

	for ( i = 0; i <= 98; i++)
	{
		sum = fib1 + fib2;
		printf("%lu, ", sum);
		fib1 = fib2;
		fib2 = sum;